NAME		= @NAME@
VERSION		= @VERSION@

srcdir		= @srcdir@
VPATH		= @srcdir@/java.util.zip:@srcdir@/java.util.zip.stubs

prefix		= @prefix@
exec_prefix	= @exec_prefix@

libdir		= @libdir@

CC		= @CC@
CFLAGS		= @CFLAGS@
EXTRA_CFLAGS	= @EXTRA_CFLAGS@
PIC		= @PIC@

AS		= @AS@
GASP		= @GASP@

LD		= @LD@
AR		= @AR@
RANLIB		= @RANLIB@

INSTALL		= @INSTALL@
INSTALL_DATA	= @INSTALL_DATA@

MKDIR		= @MKDIR@

LIBNAME		= $(NAME)_zip

LIBEXT		= @LIBEXT@
LIB		= lib$(LIBNAME)$(LIBEXT)

INCLUDES=	-I$(srcdir)/../../include -I$(srcdir)/../../src \
		-I$(srcdir) -I../../src

OBJECTS_UTIL_ZIP=\
		Adler32.o \
		CRC32.o \
		Deflater.o \
		Inflater.o \
		Adler32Stub.o \
		CRC32Stub.o \
		DeflaterStub.o \
		InflaterStub.o

OBJECTS		= $(OBJECTS_UTIL_ZIP)

all: $(LIB)

.c.o:
	$(CC) -c $(CFLAGS) $(PIC) $(EXTRA_CFLAGS) $(INCLUDES) -o $@ $<

$(LIB): $(OBJECTS)
	@LINKVMLIB@

install: $(PROG) $(LIB)
	test -d $(libdir) || $(MKDIR) $(libdir)
	$(INSTALL_DATA) $(LIB) $(libdir)/$(LIB)

clean:
	rm -f $(OBJECTS) $(LIB)

distclean: clean
	rm -f Makefile

depend: $(OBJECTS:.o=.c)
	$(CC) -MM $(INCLUDES) $^ > .depend

TAGS:

.PHONY: all install clean depend distclean

-include .depend
